<div class="ui segment">
    <div class="ui secondary menu">
        <div class="right menu">
            <button onclick="updateButton(this, '0xCL')" class="ui blue button"> <i
                    class="icon sync"></i>Aggiorna</button>
        </div>
    </div>
    <table class="ui celled table">
        <thead>
            <tr>
                <th>Data</th>
                <th>Numero Di Telefono</th>
                <th>Stato</th>
                <th>Durata</th>
            </tr>
        </thead>
        <tbody>
            <% pageData.forEach((callData) => { 
                let date = new Date(parseInt(callData.date));
                let status = "UNKNOWN";
                if(callData.type === 1) status = "INGRESSO";
                if(callData.type === 2) status = "USCITA";
                if(callData.type === 3) status = "PERSE";
                
            %>
            <tr>
                <td class="collapsing"><%= date.toLocaleString('it-IT', { timeZone: 'UTC' }) %></td>
                <td><a href="?filter=<%= callData.phoneNo %>"><%= callData.phoneNo %></a></td>
                <td><%= status %></td>
                <td><%= callData.duration %></td>
            </tr>
            <% }) %>

        </tbody>
    </table>
</div>